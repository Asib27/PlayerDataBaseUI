<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>


 <BorderPane fx:id="borderPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.asib27.playerdatabaseui.controllers.PlayerInfoController">
    <center>
      <ScrollPane BorderPane.alignment="CENTER">
         <content>
            <GridPane alignment="CENTER" prefHeight="326.0" prefWidth="300.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="122.0" minWidth="10.0" prefWidth="68.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="186.0" minWidth="10.0" prefWidth="186.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Name" />
                  <Label text="Club" GridPane.rowIndex="1" />
                  <Label text="County" GridPane.rowIndex="2" />
                  <Label text="Position" GridPane.rowIndex="3" />
                  <Label text="Salary" GridPane.rowIndex="4" />
                  <Label text="Age" GridPane.rowIndex="5" />
                  <Label text="Jursey" GridPane.rowIndex="6" />
                  <TextField promptText="Enter Name" GridPane.columnIndex="1" />
                  <TextField promptText="Enter Club Name" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField promptText="Enter Country Name" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <ComboBox fx:id="positionComboBox" prefHeight="25.0" prefWidth="188.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
                     <children>
                        <TextField promptText="Lower Range" />
                        <TextField promptText="Upper Range" />
                     </children>
                  </HBox>
                  <TextField promptText="Enter Jursey Name" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                  <TextField promptText="Enter Age Here" GridPane.columnIndex="1" GridPane.rowIndex="6" />
               </children>
            </GridPane>
         </content>
      </ScrollPane>
    </center>


    <bottom>
      <ToolBar prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <items>
            <TextField promptText="hint/error message" />
            <ComboBox fx:id="choiceComboBox" prefWidth="150.0" />
            <ComboBox fx:id="fieldComboBox" prefWidth="100.0" />
            <Button fx:id="searchButton" mnemonicParsing="false" text="Search" />
         </items>
      </ToolBar>
    </bottom>




</BorderPane>
