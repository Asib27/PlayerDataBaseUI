<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.asib27.playerdatabaseui.controllers.PlayerTableController">
   <center>
      <AnchorPane id="AnchorPane" prefHeight="400.0" styleClass="mainFxmlClass" BorderPane.alignment="CENTER">
          <stylesheets>
              <URL value="@/styles/playertable.css" />
          </stylesheets>
         <children>
            <TableView fx:id="playerTable" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy></TableView>
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </AnchorPane>
   </center>
   <top>
      <HBox prefHeight="38.0" prefWidth="248.0" BorderPane.alignment="CENTER">
         <children>
            <Pane prefHeight="38.0" prefWidth="41.0" HBox.hgrow="ALWAYS" />
            <Label contentDisplay="GRAPHIC_ONLY" text="Label">
               <graphic>
                  <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../image/others/search.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Label>
            <TextField />
            <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="65.0" text="Search" />
         </children>
      </HBox>
   </top>
</BorderPane>
