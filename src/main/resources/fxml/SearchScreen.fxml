<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns:fx="http://javafx.com/fxml/1" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/16"  fx:controller="com.asib27.playerdatabaseui.controllers.SearchScreenController">
    <stylesheets>
        <URL value="@/styles/searchscreen.css" />
    </stylesheets>
    
    <center>
        <SplitPane  fx:id="mainSplitPane" id="SplitPane" dividerPositions="0.5" prefHeight="400.0" prefWidth="600.0" >

           <items>
              <SplitPane fx:id="leftSplitPane" dividerPositions="0.5" orientation="VERTICAL" prefHeight="200.0" prefWidth="160.0" >
                <items>
                  <AnchorPane fx:id="paneLeftUp" minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0"  />
                  <AnchorPane fx:id="paneLeftDown"  minHeight="0.0" minWidth="0.0"/>
                </items>
              </SplitPane>
              <AnchorPane fx:id="paneRight"  prefHeight="200.0" prefWidth="200.0" />
           </items>
        </SplitPane>
    </center>
</BorderPane>
